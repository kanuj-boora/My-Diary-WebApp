<%- include('partials/header') %>

    <table>
        <thead>
            <tr>
                <% for(let i=0; i<posts['columns'].length; i++) { %>
                    <td>
                        <%= posts['columns'][i] %>
                    </td>
                    <% } %>
            </tr>
        </thead>
        <tbody>
            <% for(let i=0; i<posts['data'].length; i++) { %>
                <tr>
                    <% for(let j=0; j<posts['data'][i].length; j++) { %>
                        <td>
                            <%= posts['data'][i][j] %>
                        </td>
                    <% } %>
                </tr>
            <% } %>
        </tbody>
    </table>

    <div>
        <canvas id="pos-neg-chart"></canvas>
    </div>
    <%- include('partials/footer') %>